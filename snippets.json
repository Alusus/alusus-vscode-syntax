{
	"alusus module": {
		"prefix": "amodule",
		"body": [
			"import \"Srl/Console.alusus\";\n",
			"def ${1:NewModule}: module {",
			"\tdef ${2:start}: function(argCount: Int, argv: ptr[array[ptr[Char]]]) {",
			"\t\tSrl.Console.print(${3:\"Hello World\"});",
			"\t};",
			"};\n",
			"run ${1:NewModule}.${2:start};"
		],
		"description": "create new module"
	},
	"alusus print": {
		"prefix": "aprint",
		"body": [
			"Srl.Console.print(${1:\"Hello World\"});"
		],
		"description": "create new module"
	},
	"alusus for": {
		"prefix": "afor",
		"body": [
			"def ${1:index}: int;",
			"for ${1:index}=0, ${1:index}<${2:length}, ${1:index}++ {",
			"\t$3",
			"};"
		],
		"description": "create new module"
	},
	"alusus def": {
		"prefix": "adef",
		"body": [
			"def ${1:variable}: ${2:int};",
			"${1:variable} = ${3:0};"
		],
		"description": "create new module"
	},
	"alusus console print": {
		"prefix": "afunc",
		"body": [
			"def ${1:functionName}: function ($2) {",
			"\t$3",
			"};"
		],
		"description": "create new module"
	}
}