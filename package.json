{
    "name": "alusus-vscode-syntax",
    "displayName": "alusus-vscode-syntax",
    "description": "Alusus Syntax highlight with snippets",
    "publisher": "Salem",
    "version": "0.0.5",
    "engines": {
        "vscode": "^1.32.0"
    },
    "repository": {
        "type": "github",
        "url": "https://github.com/Alusus/alusus-vscode-syntax"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "alusus",
            "aliases": ["alusus", "alusus"],
            "extensions": [".alusus", ".أسس"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "alusus",
            "scopeName": "source.alusus",
            "path": "./syntaxes/alusus.tmLanguage.json"
        }],
        "snippets": [{
            "language": "alusus",
            "path": "./snippets.json"
        }]
    }
}